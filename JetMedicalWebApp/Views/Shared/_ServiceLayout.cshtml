<!DOCTYPE html>
<html lang="en">
<head>
    @{
        string seoTitle = string.Empty;
        string seoKeyword = string.Empty;
        string seoDescription = string.Empty;

        if (!string.IsNullOrEmpty(ViewBag.seoTitle))
        {
            seoTitle = ViewBag.seoTitle + " | Bệnh viện 199 Bộ Công An";
        }
        else
        {
            seoTitle = "Dịch vụ | Bệnh viện 199 Bộ Công An";
        }

        if (!string.IsNullOrEmpty(ViewBag.seoKeyword))
        {
            seoKeyword = ViewBag.seoKeyword;
        }
        else
        {
            seoKeyword = "199hospital, 199hospital da nang, Bệnh viện 199 Bộ Công An, bệnh viện 199, bv199, Bệnh viện công an đà nẵng, bệnh viện 199 đà nẵng, bv199 đà nẵng";
        }

        if (!string.IsNullOrEmpty(ViewBag.seoDescription))
        {
            seoDescription = ViewBag.seoDescription;
        }
        else
        {
            seoDescription = "Bệnh viện 199 là Bệnh viện đa khoa hạng I với 450 giường bệnh, 25 khoa, phòng cùng 412 cán bộ, công nhân viên. Bệnh viện có nhiệm vụ chăm sóc sức khỏe cho Cán bộ, chiến sĩ Công an các tỉnh, thành phố Miền Trung – Tây Nguyên và tham gia y tế cộng đồng theo quy định của Nhà nước và của Bộ Công an.";
        }
    }
    <meta http-equiv="content-type" content="text/html; charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="description" content="@Html.Raw(seoDescription)">
    <meta name="keywords" itemprop="keywords" content="@Html.Raw(seoKeyword)">
    <title>@Html.Raw(seoTitle)</title>

    <link rel="shortcut icon" href="favicon.ico" type="image/x-icon">
    <!-- Open Graph data -->
    <meta property="og:type" content="article" />
    <meta property="og:site_name" content="Bệnh viện 199 Bộ Công AN" />
    <meta property="og:title" content="@Html.Raw(seoTitle)" />
    <meta property="og:url" content="https://benhvien199.vn/@ViewBag.url" />
    <meta property="og:image" content="@ViewBag.avartar" />
    <meta property="og:description" content="@Html.Raw(seoDescription)" />
    <meta property="og:locale" content="vi_VN" />
    <meta name="format-detection" content="telephone=no">
    <meta name="thumbnail" content="@ViewBag.avartar" />

    @RenderSection("styles", required: false)
    <!-- Font Family & Font Icon -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css" integrity="sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf" crossorigin="anonymous">
    <link href="@Url.Content("~/Content/weather-icons/css/weather-icons.css")" rel="stylesheet" type="text/css">
    <!-- CSS MAIN -->
    <link rel="stylesheet" type="text/css" media="all" href="~/Content/Website/Assets/css/slick.css">
    <link rel="stylesheet" type="text/css" media="all" href="~/Content/Website/Assets/css/slick-theme.css">
    <link rel="stylesheet" type="text/css" media="all" href="~/Content/Website/Assets/css/animition.css">
    <link rel="stylesheet" type="text/css" media="all" href="~/Content/Website/Assets/css/ball.css">
    <link rel="stylesheet" type="text/css" media="all" href="~/Content/Website/Assets/css/date.css">
    <link rel="stylesheet" type="text/css" media="all" href="~/Content/Website/Assets/css/main.css">
    <link rel="stylesheet" type="text/css" media="all" href="~/Content/Website/Assets/css/menu.css">
    <link rel="stylesheet" type="text/css" media="all" href="~/Content/Website/Assets/css/menu_drop.css">
    <link rel="stylesheet" type="text/css" media="all" href="~/Content/Website/Assets/css/send_alert.css">
    <link rel="stylesheet" type="text/css" media="all" href="~/Content/Website/Assets/css/tabs.css">
    <link rel="stylesheet" type="text/css" media="all" href="~/Content/Website/Assets/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" media="all" href="~/Content/Website/Assets/css/swiper.min.css">
    <!-- Responsive -->
    <link rel="stylesheet" href="~/Content/Website/Assets/css/responsive/media.css">
    <link rel="stylesheet" href="~/Content/spinner/css/spinner.css" />
    <link href="~/Content/theme/plugins/bootstrap-timepicker/bootstrap-timepicker.min.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="@Url.Content("~/content/css/select2.min.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/css/customiseSelect2.css")" type="text/css">
    <link rel="stylesheet" type="text/css" media="all" href="~/Content/Website/View/style/tag.css">

    <!-- Script -->
    <script type="text/javascript" src="~/Content/Website/Assets/js/jquery_3.2.1.js"></script>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-FZK3M25ES5"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'G-FZK3M25ES5');
    </script>
	@*<script type="text/javascript">        
        if (window.location.protocol != "https:") {
           window.location.protocol = "https";
        }
    </script>*@
</head>

<body>
    @Html.Partial("_PartialNhanTuVanModal")
    @Html.Partial("_PartialDatLichKhamModal")
    @Html.Partial("_PartialVideoModal")
    @Html.Partial("_PartialModalRegister_Login")
    @Html.Partial("_PartialCapNhatThongTinMemberModal")
