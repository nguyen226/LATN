<div class="modal fade" id="modalUpdateNhanTuVan" tabindex="-1" role="dialog" aria-labelledby="modalUpdateRegisterService" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header vd_bg-blue vd_white">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true"><i class="fa fa-times"></i></button>
                <h4 class="modal-title" id="modalUpdateRegisterServiceLabel">@Html.Raw("Chi tiết")</h4>
            </div>
            <div class="modal-body">
                <form class="form-horizontal" id="FormUpdateRegisterService">
                    <div class="form-group">
                        <label class="col-sm-3 control-label">@Html.Raw("Họ và tên")</label>
                        <div class="col-sm-9 controls">
                            <input class="input-border-btm" id="FormUpdateNhanTuVan_HoVaTen" type="text">
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="col-sm-3 control-label">@Html.Raw("SĐT")</label>
                        <div class="col-sm-9 controls">
                            <input class="input-border-btm" id="FormUpdateNhanTuVan_SoDienThoai" type="text">
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="col-sm-3 control-label">@Html.Raw("Email")</label>
                        <div class="col-sm-9 controls">
                            <input class="input-border-btm" id="FormUpdateNhanTuVan_Email" type="text">
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="col-sm-3 control-label">@Html.Raw("Tiêu đề")</label>
                        <div class="col-sm-9 controls">
                            <input class="input-border-btm" id="FormUpdateNhanTuVan_TieuDe" type="text">
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="col-sm-3 control-label">@Html.Raw("Chi tiết")</label>
                        <div class="col-sm-9 controls">
                            <textarea class="input-border-btm" rows="10" id="FormUpdateNhanTuVan_NoiDung" type="text"></textarea>
                        </div>
                    </div>

                </form>
            </div>
            <div class="modal-footer background-login">
                <button type="button" class="btn vd_btn vd_bg-grey" data-dismiss="modal"><i class="append-icon fa fa-fw fa-ban"></i>@Html.Raw(" Đóng")</button>
            </div>
        </div>
    </div>
</div>
